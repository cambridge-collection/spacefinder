function markerColor(space_type) {
    switch (String(space_type).toLowerCase()) {
        case 'bar':
        case 'caf':
        case 'restaurant':
            return '#EA7125';
            break;
        case 'lab':
        case 'lecture room':
        case 'library space':
        case 'meeting room':
        case 'seminar room':
            return '#00B1C1';
            break;
        default:
            return '#666666';
    }
}

